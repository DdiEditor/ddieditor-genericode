      <menuContribution
            allPopups="false"
            locationURI="menu:toolsmenu?after=ddieditor-ui.file.tools.separator1">
         <separator
               name="ddieditor-genericode.separator2"
               visible="true">
         </separator>
         <command
               commandId="dk.dda.ddieditor.genericode.command.CreateCv"
               label="Create CV"
               style="push">
         </command>
         <command
